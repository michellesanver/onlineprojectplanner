<?xml version="1.0"?>
<!-- root -->
<settings>
    <!--  short description of the widget -->
	<about>A sprintplanner tool</about>
    <!-- version of widget-->
	<version>1.0</version>
    <!-- link to website -->
	<link>none</link>
    <!-- name of author -->
	<author>Michelle Sanver</author>
    <!-- icon to widget relative to main widget folder (can be left empty and a generic icon will be loaded) -->
	<icon>sprint_planner_icon.png</icon>
    <!-- title of widget; will be placed under icon (can be left empty) -->
    <icon_title>Sprint Planner</icon_title>
    <!-- namespace of widget (namespace = javascript object) -->
    <widget_object>sprint_planner</widget_object>
    <allow_multiple>no</allow_multiple>
    <!--
        list of files to be loaded relative to main folder of widgets
        (theese files will be loaded upon project start)
        
        valid types are; javascript, css
    -->
	<load>
	
        <file type="javascript">js/sprint_planner.js</file>
		<file type="javascript">js/jquery.jqplot.js</file>
        
        <file type="javascript">js/jqplot.cursor.js</file>
        <file type="javascript">js/jqplot.pieRenderer.js</file>
        <file type="javascript">js/jqplot.ohlcRenderer.js</file>
        <file type="javascript">js/jqplot.categoryAxisRenderer.js</file>
        <file type="javascript">js/jqplot.dateAxisRenderer.js</file>    
        <file type="javascript">js/jqplot.canvasTextRenderer.min.js</file>
        <file type="javascript">js/jqplot.canvasAxisLabelRenderer.min.js</file>
        <file type="javascript">js/jqplot.canvasAxisTickRenderer.js</file> 
        <file type="javascript">js/jqplot.highlighter.js</file>  
        <file type="javascript">js/jqplot.cursor.js</file> 
  		
  		
  		  
            
        <file type="css">css/sprint_planner.css</file>
        <file type="css">css/jquery.jqplot.css</file>
    </load>
   
</settings>