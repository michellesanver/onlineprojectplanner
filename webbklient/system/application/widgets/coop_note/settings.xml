<?xml version="1.0"?>
<!-- root -->
<settings>
    <!--  short description of the widget -->
	<about>Write notes or code together with your associates.</about>
	
    <!-- version of widget-->
	<version>1.0</version>
	
    <!-- link to website -->
	<link>none</link>
	
    <!-- name of author -->
	<author>Dennis Sangmo</author>
	
    <!-- icon to widget relative to main widget folder (can be left empty and a generic icon will be loaded) -->
	<icon>notepad.png</icon>
	
    <!-- title of widget; will be placed under icon (can be left empty) -->
    <icon_title>Coop Notes</icon_title>
	
    <!-- namespace of widget (namespace = javascript object) -->
    <widget_object>coopNote</widget_object>
	<in_development>true</in_development>
	
    <!--
        list of files to be loaded relative to main folder of widgets
        (theese files will be loaded upon project start)
        
        valid types are; javascript, css
    -->
	<load>
        <file type="javascript">js/coopNote.js</file>
		<file type="javascript">views/coop_note_view.js</file>
		<file type="javascript">js/field_selection.js</file>
        <file type="css">css/style.css</file>
    </load>
</settings>